{
  "name": "solana-plugin-para",
  "description": "para plugin for solana agent kit v2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "build:plugin-para-web": "turbo run build --filter=solana-plugin-para-web",
    "build:plugin-para-server": "turbo run build --filter=solana-plugin-para-server",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky",
    "version-packages": "changeset version",
    "publish-packages": "turbo run build && changeset publish",
    "publish-para-plugins": "turbo run build --filter=solana-plugin-para-web --filter=solana-plugin-para-server && changeset publish"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "solana",
    "agent",
    "ai",
    "solana agent kit",
    "para"
  ],
  "author": "uratmangun",
  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "turbo": "^2.3.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.15.3"
}
